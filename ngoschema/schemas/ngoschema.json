{
  "$id": "https://numengo.org/ngoschema#",
  "type": "object",
  "$comment": "automatically generated for ngoschema",
  "$defs": {
    "contexts": {
      "type": "object",
      "$defs": {
        "NsManagerContext": {
          "type": "object",
          "notSerialized": [
            "_nsMgr"
          ],
          "notValidated": [
            "_nsMgr"
          ],
          "properties": {
            "_nsMgr": true
          }
        },
        "ObjectProtocolContext": {
          "type": "object",
          "notSerialized": [
            "_parent"
          ],
          "notValidated": [
            "_parent"
          ],
          "extends": [
            "#/$defs/contexts/$defs/NsManagerContext"
          ],
          "properties": {
            "_parent": true
          }
        },
        "InstanceContext": {
          "type": "object",
          "notSerialized": [
            "_parentInstance"
          ],
          "notValidated": [
            "_parentInstance"
          ],
          "extends": [
            "#/$defs/contexts/$defs/ObjectProtocolContext"
          ],
          "properties": {
            "_parentInstance": true
          }
        }
      }
    },
    "metadata": {
      "type": "object",
      "$defs": {
        "Annotation": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "$comment": {
              "type": "string"
            }
          }
        },
        "Name": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            }
          }
        },
        "Id": {
          "type": "object",
          "properties": {
            "$id": {
              "type": "id"
            },
            "$schema": {
              "type": "id"
            }
          }
        },
        "Tag": {
          "type": "object",
          "extends": [
            "#/$defs/metadata/$defs/Annotation"
          ],
          "properties": {
            "tags": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/metadata/$defs/Tag"
              }
            }
          }
        },
        "Plural": {
          "type": "object",
          "properties": {
            "namePlural": {
              "type": "string",
              "default": "{%if this.name %}{{this.name|pluralize}}{% endif %}"
            }
          }
        }
      }
    },
    "protocols": {
      "type": "object",
      "$defs": {
        "Context": {
          "type": "object"
        },
        "Checker": {
          "type": "object",
          "properties": {
            "pyType": {
              "type": "importable"
            }
          }
        },
        "Converter": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Checker"
          ],
          "properties": {
            "checker": {
              "$ref": "#/$defs/protocols/$defs/Checker"
            }
          }
        },
        "Transformer": {
          "type": "object",
          "aliases": {
            "fromClass": "pyType"
          },
          "extends": [
            "#/$defs/protocols/$defs/Converter"
          ],
          "properties": {
            "converter": {
              "$ref": "#/$defs/protocols/$defs/Converter"
            },
            "toClass": {
              "type": "importable"
            }
          }
        },
        "Validator": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Converter"
          ],
          "properties": {
            "schema": {
              "type": "string"
            },
            "errors": {
              "type": "object"
            },
            "converter": {
              "$ref": "#/$defs/protocols/$defs/Converter"
            }
          }
        },
        "Deserializer": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Validator"
          ],
          "properties": {
            "many": {
              "type": "boolean"
            },
            "validator": {
              "$ref": "#/$defs/protocols/$defs/Validator"
            }
          }
        },
        "Serializer": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Deserializer"
          ],
          "properties": {
            "deserializer": {
              "$ref": "#/$defs/protocols/$defs/Deserializer"
            }
          }
        },
        "Loader": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Deserializer"
          ],
          "properties": {
            "instanceClass": {
              "type": "importable"
            },
            "deserializer": {
              "$ref": "#/$defs/protocols/$defs/Deserializer"
            }
          }
        },
        "Resolver": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Converter"
          ],
          "properties": {
            "converter": {
              "$ref": "#/$defs/protocols/$defs/Converter"
            }
          }
        },
        "Saver": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Loader"
          ],
          "properties": {
            "serializer": {
              "$ref": "#/$defs/protocols/$defs/Serializer"
            },
            "loader": {
              "$ref": "#/$defs/protocols/$defs/Loader"
            }
          }
        },
        "Repository": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Saver"
          ],
          "properties": {
            "content": true,
            "saver": {
              "$ref": "#/$defs/protocols/$defs/Saver"
            }
          }
        }
      }
    },
    "managers": {
      "type": "object",
      "$defs": {
        "TypeBuilder": {
          "type": "object"
        },
        "NamespaceManager": {
          "type": "object"
        }
      }
    },
    "relationships": {
      "type": "object",
      "$defs": {
        "Relationship": {
          "type": "object",
          "additionalProperties": false,
          "notSerialized": [
            "foreignClass"
          ],
          "readOnly": [
            "foreignClass"
          ],
          "dependencies": {
            "foreignKeys": [
              "foreignClass"
            ],
            "foreignClass": [
              "foreignSchema"
            ]
          },
          "extends": [
            "#/$defs/metadata/$defs/Name"
          ],
          "properties": {
            "foreignSchema": {
              "type": "id"
            },
            "foreignClass": {
              "type": "importable"
            },
            "foreignKeys": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "cardinality": {
              "type": "string",
              "enum": [
                "one2one",
                "one2many"
              ]
            },
            "ordering": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "reverse": {
              "type": "boolean"
            },
            "inheritance": {
              "type": "boolean"
            }
          }
        },
        "ForeignKey": {
          "type": "object",
          "additionalProperties": false,
          "extends": [
            "#/$defs/relationships/$defs/Relationship"
          ],
          "properties": {
            "backPopulates": {
              "$ref": "#/$defs/relationships/$defs/Relationship"
            },
            "onDelete": {
              "type": "string",
              "enum": [
                "CASCADE",
                "PROTECT",
                "SET_NULL",
                "SET_DEFAULT",
                "SET",
                "DO_NOTHING"
              ]
            }
          }
        }
      }
    },
    "instances": {
      "type": "object",
      "$defs": {
        "Instance": {
          "type": "object",
          "additionalProperties": false,
          "abstract": true,
          "notSerialized": [
            "canonicalName"
          ],
          "extends": [
            "#/$defs/contexts/$defs/InstanceContext",
            "#/$defs/metadata/$defs/Name",
            "#/$defs/metadata/$defs/Annotation"
          ],
          "properties": {
            "name": {
              "type": "string"
            },
            "canonicalName": {
              "type": "string"
            },
            "properties": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/instances/$defs/Instance"
              }
            }
          }
        },
        "InstanceList": {
          "type": "object",
          "notSerialized": [
            "items",
            "namePlural"
          ],
          "extends": [
            "#/$defs/instances/$defs/Instance",
            "#/$defs/metadata/$defs/Plural"
          ],
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/instances/$defs/Instance"
              }
            }
          }
        },
        "Entity": {
          "type": "object",
          "additionalProperties": false,
          "abstract": true,
          "notSerialized": [
            "identityKeys",
            "primaryKeys",
            "$schema",
            "$id"
          ],
          "readOnly": [
            "identityKeys"
          ],
          "dependencies": {},
          "extends": [
            "#/$defs/instances/$defs/Instance",
            "#/$defs/metadata/$defs/Id"
          ],
          "properties": {
            "primaryKeys": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "identityKeys": {
              "type": "array"
            }
          }
        }
      }
    },
    "files": {
      "type": "object",
      "$defs": {
        "File": {
          "type": "object",
          "primaryKeys": [
            "filepath"
          ],
          "extends": [
            "#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "filepath": {
              "type": "path"
            },
            "binary": {
              "type": "boolean"
            },
            "contentRaw": true
          }
        },
        "FileInfo": {
          "type": "object",
          "required": [
            "file"
          ],
          "properties": {
            "file": {
              "$ref": "#/$defs/files/$defs/File"
            },
            "dateCreated": {
              "type": "datetime"
            },
            "dateModified": {
              "type": "datetime"
            },
            "contentSize": {
              "type": "integer"
            },
            "contentSizeHuman": {
              "type": "string"
            },
            "mimetype": {
              "type": "string"
            },
            "sha1": {
              "type": "string"
            }
          }
        },
        "UriFile": {
          "type": "object",
          "primaryKeys": [
            "uri"
          ],
          "extends": [
            "#/$defs/files/$defs/File"
          ],
          "properties": {
            "uri": {
              "type": "uri"
            },
            "contentRaw": true
          }
        },
        "Document": {
          "type": "object",
          "additionalProperties": false,
          "primaryKeys": [
            "identifier"
          ],
          "extends": [
            "#/$defs/files/$defs/File"
          ],
          "properties": {
            "encoder": {
              "$ref": "#/$defs/serializers/$defs/JsonSerializer"
            },
            "file_info": {
              "$ref": "#/$defs/files/$defs/FileInfo"
            },
            "identifier": {
              "type": "string"
            },
            "charset": {
              "type": "string",
              "default": "utf-8"
            },
            "version": {
              "type": "string"
            },
            "author": {
              "type": "string"
            },
            "keywords": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "content": true,
            "doc_id": {
              "type": "string"
            }
          }
        }
      }
    },
    "serializers": {
      "type": "object",
      "$defs": {
        "CollectionSerializer": {
          "type": "object",
          "properties": {
            "many": {
              "type": "boolean"
            },
            "excludes": {
              "type": "array"
            },
            "only": {
              "type": "array"
            }
          }
        },
        "ObjectSerializer": {
          "type": "object",
          "extends": [
            "#/$defs/serializers/$defs/CollectionSerializer"
          ],
          "properties": {
            "attrPrefix": {
              "type": "string"
            },
            "noDefaults": {
              "type": "boolean"
            },
            "noReadOnly": {
              "type": "boolean"
            }
          }
        },
        "InstanceSerializer": {
          "type": "object",
          "extends": [
            "#/$defs/serializers/$defs/ObjectSerializer"
          ],
          "properties": {
            "tag": {
              "type": "string"
            }
          }
        },
        "EntitySerializer": {
          "type": "object",
          "extends": [
            "#/$defs/serializers/$defs/InstanceSerializer"
          ],
          "properties": {
            "useEntityKeys": {
              "type": "boolean"
            },
            "useIdentityKeys": {
              "type": "boolean"
            }
          }
        },
        "FileSerializer": {
          "type": "object",
          "extends": [
            "#/$defs/protocols/$defs/Serializer"
          ],
          "properties": {
            "filepath": {
              "type": "path"
            },
            "binary": {
              "type": "boolean"
            },
            "charset": {
              "type": "string",
              "default": "utf-8"
            }
          }
        },
        "JsonSerializer": {
          "type": "object",
          "extends": [
            "#/$defs/serializers/$defs/InstanceSerializer"
          ]
        },
        "XmlSerializer": {
          "type": "object",
          "extends": [
            "#/$defs/serializers/$defs/InstanceSerializer"
          ],
          "properties": {
            "attr_prefix": {
              "type": "string",
              "default": "@"
            },
            "pretty": {
              "type": "boolean",
              "default": true
            },
            "cdata_key": {
              "type": "string",
              "default": "#text"
            },
            "short_empty_elements": {
              "type": "boolean",
              "default": true
            },
            "indent": {
              "type": "string",
              "default": "\t"
            },
            "newl": {
              "type": "string",
              "default": "\n"
            }
          }
        },
        "YamlSerializer": {
          "type": "object",
          "extends": [
            "#/$defs/serializers/$defs/InstanceSerializer"
          ]
        }
      }
    },
    "repositories": {
      "type": "object",
      "$defs": {
        "MemoryRepository": {
          "type": "object",
          "additionalProperties": false,
          "extends": [
            "#/$defs/protocols/$defs/Repository",
            "#/$defs/serializers/$defs/EntitySerializer"
          ],
          "properties": {
            "instanceClass": {
              "type": "importable"
            }
          }
        },
        "FileRepository": {
          "type": "object",
          "additionalProperties": false,
          "extends": [
            "#/$defs/protocols/$defs/Repository",
            "#/$defs/serializers/$defs/FileSerializer"
          ],
          "properties": {
            "overwrite": {
              "type": "boolean"
            }
          }
        },
        "JsonFileRepository": {
          "type": "object",
          "extends": [
            "#/$defs/repositories/$defs/FileRepository",
            "#/$defs/serializers/$defs/JsonSerializer"
          ]
        },
        "YamlFileRepository": {
          "type": "object",
          "additionalProperties": true,
          "extends": [
            "#/$defs/repositories/$defs/FileRepository",
            "#/$defs/serializers/$defs/YamlSerializer"
          ]
        },
        "XmlFileRepository": {
          "type": "object",
          "additionalProperties": true,
          "extends": [
            "#/$defs/repositories/$defs/FileRepository",
            "#/$defs/serializers/$defs/XmlSerializer"
          ],
          "properties": {
            "postprocessor": {
              "type": "importable"
            }
          }
        }
      }
    },
    "queries": {
      "type": "object",
      "$defs": {
        "Filter": {
          "type": "object",
          "additionalProperties": true,
          "properties": {
            "attrs": {
              "type": "array",
              "items": true
            },
            "attrs_value": {
              "type": "object"
            },
            "negate": {
              "type": "boolean"
            },
            "any_of": {
              "type": "boolean"
            }
          }
        },
        "Query": {
          "type": "object",
          "notSerialized": [
            "iterable"
          ],
          "readOnly": [
            "result"
          ],
          "notValidated": [
            "iterable"
          ],
          "extends": [
            "#/$defs/queries/$defs/Filter"
          ],
          "properties": {
            "iterable": {
              "type": "array"
            },
            "distinct": {
              "type": "boolean"
            },
            "order_by": true,
            "reverse": {
              "type": "boolean"
            },
            "result": {
              "type": "array"
            }
          }
        },
        "EntityQuery": {
          "type": "object",
          "readOnly": [
            "iterable"
          ],
          "extends": [
            "#/$defs/queries/$defs/Query"
          ],
          "properties": {
            "entity": {
              "type": "importable"
            },
            "field": {
              "type": "string"
            }
          }
        }
      }
    },
    "session": {
      "type": "object",
      "$defs": {
        "Session": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "autoflush": {
              "type": "boolean",
              "default": true
            },
            "expire_on_commit": {
              "type": "boolean",
              "default": true
            },
            "autocommit": {
              "type": "boolean",
              "default": true
            },
            "bind": {
              "type": "object",
              "additionalProperties": true
            },
            "repositories": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/protocols/$defs/Repository"
              }
            }
          }
        }
      }
    }
  }
}